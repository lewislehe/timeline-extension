{
    "manifest_version": 3,
    "name": "UIUC Last Mile study timeline share tool",
    "version": "0.1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "block-i-orange-background-16.png",
            "48": "block-i-orange-background-48.png",
            "128": "block-i-orange-background-128.png"
        },
        "default_title": "UIUC Last Mile study timeline share tool"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://*.google.com/",
        "https://*.transitstudy.web.illinois.edu/"
    ]
}